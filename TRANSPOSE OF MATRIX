#include <stdio.h>
int main()
{
    int A[3][3]={1,2,3,4,5,6,7,8,9};
    int result[3][3]={0,0,0,0,0,0,0,0,0};
    int i,j;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            result[i][j]=A[j][i];
        }
    }
        for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
           printf("%d\t",result[i][j]);
        }
        printf("\n");
    }
    return 0;
}
