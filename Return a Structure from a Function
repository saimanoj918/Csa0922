#include<stdio.h>
struct student
{
    char name[50];
    int age;
    float marks;
};
struct student createstudent()
{
    struct student s={"Manoj",18,99.99};
    return s;
}
int main()
{
    struct student s=createstudent();
    printf("student details: Name:%s,Age:%d,Marks:%.2f",s.name,s.age,s.marks);
    return 0;
}
