#include <stdio.h>
int main() 
{
    int n=3, i, count = 0;
    for (i = 1; i <= n; i++) 
    {
        if (n % i == 0) 
        {
            count++;
        }
    }
    if (count == 2) 
    {
        printf("%d is a Prime number",n);
    } 
    else 
    {
        printf("%d is Not a prime number",n);
    }
    return 0;
}
