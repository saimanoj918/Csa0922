#include <stdio.h>
int main()
{
    int A[3][3]={1,2,3,4,5,6,7,8,9};
    int B[3][3]={9,8,7,6,5,4,3,2,1};
    int mul[3][3]={0,0,0,0,0,0,0,0,0};
    int i,j,k;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            int sum=0;
            for(k=0;k<3;k++)
            {
                sum +=A[i][k]*B[k][j];
            }
            mul[i][j]=sum;
        }
    }
        for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
           printf("%d\t",mul[i][j]);
        }
        printf("\n");
    }
    return 0;
}
