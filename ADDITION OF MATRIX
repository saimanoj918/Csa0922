#include <stdio.h>
int main()
{
    int A[2][2]={1,5,7,3};
    int B[2][2]={12,1,5,9};
    int sum [2][2]={0,0,0,0};
    int i,j;
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            sum[i][j]=A[i][j]+B[i][j];
        }
    }
        for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
           printf("%d\t",sum[i][j]);
        }
        printf("\n");
    }
    return 0;
}
